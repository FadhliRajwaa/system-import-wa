{
    "$schema": "https://www.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "node --max-old-space-size=4096 node_modules/vite/bin/vite.js",
        "dev:server": "php artisan serve",
        "dev:queue": "php artisan queue:work --tries=1 --memory=128",
        "dev:all": "concurrently -c \"#93c5fd,#c4b5fd,#fdba74\" \"npm run dev:server\" \"npm run dev:queue\" \"npm run dev\" --names=server,queue,vite"
    },
    "dependencies": {
        "@iconify-json/heroicons": "^1.2.3",
        "@iconify-json/ph": "^1.2.2",
        "@tailwindcss/vite": "^4.1.11",
        "animate.css": "^4.1.1",
        "autoprefixer": "^10.4.20",
        "axios": "^1.7.4",
        "concurrently": "^9.0.1",
        "framer-motion": "^12.23.26",
        "laravel-vite-plugin": "^2.0",
        "tailwindcss": "^4.0.7",
        "vite": "^7.0.4"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "4.9.5",
        "@tailwindcss/oxide-linux-x64-gnu": "^4.0.1",
        "lightningcss-linux-x64-gnu": "^1.29.1"
    }
}
